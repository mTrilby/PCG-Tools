                    <CheckBox x:Name="checkBoxStatusExt" Content="{x:Static common:Strings.TimbreExt_setw}" Margin="4"/>
                    <CheckBox x:Name="checkBoxStatusEx2" Content="{x:Static common:Strings.TimbreEx2_setw}" Margin="4"/>
                </StackPanel>
            </StackPanel>
        <TabItem!-->

