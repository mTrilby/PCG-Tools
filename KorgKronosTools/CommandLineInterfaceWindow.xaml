<Window x:Class="PcgTools.CommandLineInterfaceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:common="clr-namespace:Common.PcgToolsResources;assembly=Common"
        Title="{x:Static common:Strings.CliWindow}"
        Height="486" Width="719"
        Loaded="Window_Loaded"
        WindowStartupLocation="CenterOwner"
        ShowInTaskbar="False">
    <Grid>
        <ScrollViewer HorizontalAlignment="Stretch" Margin="0,0,0,29" Name="scrollViewer1" VerticalAlignment="Stretch">
            <TextBlock HorizontalAlignment="Stretch" Name="textBlock" Text="" VerticalAlignment="Stretch" Margin="0,0,0,0" />
        </ScrollViewer>
        <Button Content="Close" Height="23" HorizontalAlignment="Right" Margin="0,0,0,0" Name="buttonClose"
                VerticalAlignment="Bottom" Width="75" Click="ButtonCloseClick" />
    </Grid>
</Window>
