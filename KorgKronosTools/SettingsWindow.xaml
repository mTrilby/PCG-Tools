            <TabItem Header="{x:Static common:Strings.Categories_setw}">
                <GroupBox Header="{x:Static common:Strings.CategorySetSelect_setw}"
                          Margin="0,4,0,0" Height="77" VerticalAlignment="Top">
                    <StackPanel>
                        <RadioButton x:Name="rbCategoryA"
                                     Content="{x:Static common:Strings.CategorySetA_setw}" Margin="4" />
                        <RadioButton x:Name="rbCategoryB"
                                     Content="{x:Static common:Strings.CategorySetB_setw}" Margin="4" />

                    </StackPanel>
                </GroupBox>
            </TabItem>

        </TabControl>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Content="{x:Static common:Strings.Cancel}" Height="23"
                    HorizontalAlignment="Left" VerticalAlignment="Top" Margin="8,0,0,8" Width="75"
                    IsCancel="True" Click="ButtonCancelClick" />
            <Button Content="{x:Static common:Strings.OK}" Height="23"
                    HorizontalAlignment="Left" VerticalAlignment="Top" Margin="8,0,8,8" Width="75"
                    IsDefault="True" Click="ButtonOkClick" />
        </StackPanel>
    </StackPanel>
</Window>